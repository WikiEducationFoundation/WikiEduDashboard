{
  "extends": "airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "func-names": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-return-assign": 0,
    "no-extend-native": 0,
    "react/prefer-es6-class": 0,
    "react/prefer-stateless-function": 0,
    "arrow-body-style": 0,
    "max-len": 0
  },
  "globals": {
    "$": true
  }
}
